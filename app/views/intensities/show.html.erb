<div class="page-header">
  <%= link_to intensities_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_intensity_path(@intensity), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show intensity</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @intensity.name %></dd>

  <dt>Description:</dt>
  <dd><%= @intensity.description %></dd>

</dl>
